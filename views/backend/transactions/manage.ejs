		<script>
			// Function to toggle the visibility of the user select dropdown
			function toggleUserSelect() {
				if ($('#sendToAllCheckbox').is(':checked')) {
					$('#userSelectLi').hide(); // Hide the user select if the checkbox is checked
				} else {
					$('#userSelectLi').show(); // Show the user select if the checkbox is unchecked
				}
			}

			// Run the function on page load to ensure correct initial state
			$(document).ready(function() {
				toggleUserSelect();

				// Attach change event listener to the checkbox
				$('#sendToAllCheckbox').change(function() {
					toggleUserSelect();
				});
			});

		</script>
